@Html.Partial("_Stepper")
<div class="row">
    <div class="col-md-6 left">
        <div class="row">
            <div class="col-lg-4">
            </div>
            <div class="col-lg-7">
                <div class="form-group col-title">PENSION FUND REGISTER</div>
                <div class="form-group bmd-form-group">
                    <label for="fundNameInput" class="bmd-label-static">FUND NAME</label>
                    <input type="text" class="form-control" id="fundNameInput" placeholder="Enter fund name">
                </div>
                <div class="form-group bmd-form-group">
                    <label for="fundFeeInput" class="bmd-label-static">FUND FEE</label>
                    <input type="number" class="form-control" id="fundFeeInput" placeholder="00%">
                </div>
                <div class="form-group bmd-form-group">
                    <label for="latePenaltyInput" class="bmd-label-static">LATE PENALTY</label>
                    <input type="number" class="form-control" id="latePenaltyInput" placeholder="00%">
                </div>
            </div>
            <div class="col-lg-1">
            </div>
        </div>
    </div>
    <div class="col-md-6 right">
        <div class="row">
            <div class="col-lg-1">
            </div>
            <div class="col-lg-7">
                <div class="form-group col-title">ASSETS DISTRIBUTION</div>
                <div class="form-group bmd-form-group asset-input-group">
                    <span class="asset-input-group-left">
                    </span>
                    <div class="asset-input-group-label">
                        GOLD
                    </div>
                    <input />
                    <div class="asset-input-group-percent-label">%</div>
                </div>
                <div class="form-group bmd-form-group asset-input-group">
                    <span class="asset-input-group-left">
                    </span>
                    <div class="asset-input-group-label">
                        S&P
                    </div>
                    <input />
                    <div class="asset-input-group-percent-label">%</div>
                </div>
                <div class="form-group bmd-form-group asset-input-group">
                    <span class="asset-input-group-left">
                    </span>
                    <div class="asset-input-group-label">
                        BONDS
                    </div>
                    <input />
                    <div class="asset-input-group-percent-label">%</div>
                </div>
                <div class="form-group bmd-form-group asset-input-group">
                    <span class="asset-input-group-left">
                    </span>
                    <div class="asset-input-group-label">
                        MSCI WORLD
                    </div>
                    <input />
                    <div class="asset-input-group-percent-label">%</div>
                </div>
                <div class="form-group bmd-form-group asset-input-group">
                    <span class="asset-input-group-left">
                    </span>
                    <div class="asset-input-group-label">
                        BTC
                    </div>
                    <input />
                    <div class="asset-input-group-percent-label">%</div>
                </div>
            </div>
            <div class="col-lg-4">
            </div>
        </div>
    </div>
</div>

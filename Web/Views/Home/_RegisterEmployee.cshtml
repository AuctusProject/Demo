<form  id="registerEmployee">
        <div class="row">
            <div class="col-lg-12 left">
                <div class="row">
                    <div class="col-lg-2">
                    </div>
                    <div class="col-lg-8">
                        <div class="form-group col-title">EMPLOYEE REGISTER</div>
                        <div class="form-group bmd-form-group">
                            <label for="employeeNameInput" class="bmd-label-static">EMPLOYEE NAME</label>
                            <input type="text" class="form-control" id="employeeNameInput" placeholder="Enter employee name" />
                        </div>
                        <div class="form-group bmd-form-group">
                            <label for="salaryInput" class="bmd-label-static">SALARY</label>
                            <input type="number" min="0" max="100" class="form-control" id="salaryInput" placeholder="USD 000.00"
                                   data-toggle="popover"
                                   data-trigger="focus"
                                   data-placement="right"
                                   title="SALARY"
                                   data-content="<div>This fee covers the cost of administration, and are usually included in the annual management charge. However, older pensions may have a separate policy fee.</div><br/><div><strong>Choose a value between 0-100%.</strong></div>"
                                   data-html="true">
                        </div>
                        <div class="form-group bmd-form-group">
                            <label for="contributionInput" class="bmd-label-static">CONTRIBUTION</label>
                            <input type="number" min="0" max="10" class="form-control" id="contributionInput" placeholder="00%"
                                   data-toggle="popover"
                                   data-trigger="focus"
                                   data-placement="right"
                                   title="CONTRIBUTION"
                                   data-content="<div>This fee covers the cost of administration, and are usually included in the annual management charge. However, older pensions may have a separate policy fee.</div><br/><div><strong>Choose a value between 0-100%.</strong></div>"
                                   data-html="true">
                        </div>
                    </div>
                    <div class="col-lg-2">
                    </div>
                </div>
            </div>
            <div class="col-lg-12">
                <div class="back-button prev-step" >
                    <span class="next-button-icon"></span>
                </div>
                <div class="deploy-contract-button " @*disabled="disabled"*@>
                    <span class="deploy-contract-label">DEPLOY CONTRACT</span>
                </div>
            </div>
        </div>


